>\n\n    <div class=\"d-flex mt-4 justify-content-center\">\n      <router-link class=\"mx-1\" to=\"/login\"> 取消 </router-link>\n    </div>\n  </div>\n</template>\n\n<style scoped>\n.form-wrapper {\n  height: 54px;\n}\n</style>\n\n<script>\nexport default {\n  data() {\n    return {\n      account: \"\",\n      name: \"\",\n      email: \"\",\n      password: \"\",\n      passwordCheck: \"\",\n    };\n  },\n  methods: {\n    handleSubmit() {\n      const data = {\n        account: this.account,\n        name: this.name,\n        email: this.email,\n        password: this.passeord,\n        passwordCheck: this.passwordCheck,\n      };\n      console.log(JSON.stringify(data))\n    },\n  },\n};\n</script>"]}]}